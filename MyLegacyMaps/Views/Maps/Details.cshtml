@model MyLegacyMaps.Models.Map
@{
    ViewBag.Title = "Map Details";
}

@using (Html.BeginForm("Create", "AdoptedMaps", FormMethod.Post))
{ 
    @Html.Partial("~/Views/Shared/Partials/MapCanvas.cshtml")
    @Html.AntiForgeryToken();
    @Html.Hidden("mapId", Model.MapId);
