@model MyLegacyMaps.Models.Map
<div class="container-fluid">
    <div class="fooboo row-fluid">
        <div id="toolbar" class="span4">
            <h3>@Html.DisplayFor(model => Model.Name)</h3>          
            <div class="zoom-buttons">
                <a href="#" id="zoomIn">Zoom In</a>&nbsp;
                <a href="#" id="zoomOut">Zoom Out</a>
                <input id="zoomSlider" type="range" min="1.0" max="1.5" step=".0005">
                <a href="#" id="zoomReset">Reset</a>
            </div>
        </div>
        <div id="canvas"></div>
    </div>
</div>



